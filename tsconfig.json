{
    "typedocOptions": {
        "plugin": [
            "typedoc-plugin-markdown", "./generators/santiago/hugo_plugin.mjs"
        ],
        "out": "./docs/santiago/scripting",
        "entryFileName": "_index",
        "router": "structure-dir",
        "useCodeBlocks": true,
        "parametersFormat": "table",
        "interfacePropertiesFormat": "table",
        "classPropertiesFormat": "table",
        "typeAliasPropertiesFormat": "table",
        "enumMembersFormat": "table",
        "propertyMembersFormat": "table",
        "typeDeclarationFormat": "table"
    },
    "compilerOptions": {
        "target": "ES2020",
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "strict": true,
        "outDir": "dist",
        "types": [
            "./generators/santiago/mod"
        ],
        "paths": {
            "modlib": [
                "./generators/santiago/modlib"
            ]
        }
    }
}